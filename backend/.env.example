# Backend port
PORT=4000

# Your frontend origin for CORS
FRONTEND_ORIGIN=http://localhost:3000

# Supabase Postgres connection (get from Supabase -> Project Settings -> Database)
SPRING_DATASOURCE_URL=jdbc:postgresql://<your-supabase-db-host>:5432/postgres
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=<your-db-password>

# JWT secret for your API tokens (make it long and random)
JWT_SECRET=change-me-to-a-long-random-string
JWT_EXP_MINUTES=43200
